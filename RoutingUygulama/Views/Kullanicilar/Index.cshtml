@model IEnumerable<Kullanici>
@{
    ViewBag.Title = "Kullanicilar";
}

@if (
   !string.IsNullOrEmpty(ViewBag.Mesaj)
)
{
    <div class="alert alert-success">
        @ViewBag.Mesaj
    </div>
}
<div class="d-flex justify-content-between align-items-center">
    <h1>Kullanicilar</h1>
    <a asp-action="Yeni" class="btn btn-primary"> + </a>

</div>

<table class="table">
    <thead>
        <tr>

            <th>Yas</th>
            <th>Daire No</th>
            <th>Sicil No</th>

        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Yas</td>
                <td>@item.DaireNo</td>
                <td>@item.SicilNo</td>
            </tr>
        }
    </tbody>

</table>
